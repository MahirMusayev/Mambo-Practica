@using MambaPractica.Models
@model List<Team>
<h5 class="card-title fw-semibold mb-4">Recent Transactions</h5>
<a asp-action="Create" class="btn btn-success m-1">Create</a>
<div class="table-responsive">
	<table class="table text-nowrap mb-3 align-middle">
		<thead class="text-dark fs-4">
			<tr>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">Id</h6>
				</th>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">Name</h6>
				</th>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">profession</h6>
				</th>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">Image</h6>
				</th>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">Icons</h6>
				</th>
				<th class="border-bottom-0">
					<h6 class="fw-semibold mb-0">Assent</h6>
				</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{

				<tr>
					<td class="border-bottom-0">
						@item.Id
					</td>
					<td class="border-bottom-0">
						@item.Name
					</td>
					<td class="border-bottom-0">
						@item.profession
					</td>
					<td class="border-bottom-0">
						<img width="50px" height="50px" src="@item.ImageUrl" />
					</td>
					<td class="border-bottom-0">

						<i class="@item.Icons"></i>
					</td>
					<td class="border-bottom-0">
						<a asp-action="Update" asp-route-id="@item.Id" class="badge bg-primary rounded-3 fw-semibold">Update</a>
						<a asp-action="Delete" asp-route-id="@item.Id" class="badge bg-danger rounded-3 fw-semibold">Delete</a>

					</td>
				</tr>
			}

		</tbody>
	</table>
</div>